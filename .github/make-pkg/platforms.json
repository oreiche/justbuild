{ "debian11":
  { "type": "deb"
  , "image": "debian:bullseye"
  , "build-depends":
    [ "clang"
    , "protobuf-compiler"
    , "protobuf-compiler-grpc"
    , "libprotobuf-dev"
    , "libgrpc++-dev"
    , "libfmt-dev"
    , "nlohmann-json3-dev"
    , "libgit2-dev"
    , "libssl-dev"
    , "libarchive-dev"
    , "libc-ares-dev"
    , "zlib1g-dev"
    , "python3"
    , "pandoc"
    , "patch"
    , "unzip"
    , "pkg-config"
    ]
  , "non-local-deps":
    [ "cli11"
    , "com_github_microsoft_gsl"
    , "com_github_curl_curl"
    , "google_apis"
    , "bazel_remote_apis"
    ]
  }
