{ "defaults":
  { "type": ["CC", "defaults"]
  , "base": [["@", "base", "CC", "defaults"]]
  , "ADD_CFLAGS":
    [ "-std=c99"
    , "-Wall"
    , "-Wextra"
    , "-DHAVE_STDBOOL_H"
    , "-DHAVE__BOOL"
    , "-DHAVE_STDINT_H"
    , "-DHAVE_INTTYPES_H"
    , "-DHAVE_CHECK_CRC32"
    , "-D_GNU_SOURCE"
    , "-D__EXTENSIONS__"
    , "-D_POSIX_PTHREAD_SEMANTICS"
    , "-D_TANDEM_SOURCE"
    , "-D_ALL_SOURCE"
    ]
  }
}
